{
  "name": "mystical",
  "description": "CSS prop constraint based styling",
  "version": "10.0.0",
  "author": "David Burles",
  "license": "MIT",
  "repository": "github:dburles/mystical",
  "homepage": "https://github.com/dburles/mystical#readme",
  "bugs": "https://github.com/dburles/mystical/issues",
  "keywords": [
    "mystical",
    "css",
    "css-in-js",
    "styles",
    "react"
  ],
  "engines": {
    "node": "^12.20 || >= 14.13"
  },
  "browserslist": [
    "> 0.5%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./public/index.mjs",
      "require": "./public/index.js"
    },
    "./public/*": "./public/*",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "files": [
    "index.d.ts",
    "public",
    "private"
  ],
  "main": "public",
  "types": "index.d.ts",
  "scripts": {
    "prepare": "npm run prepare:clean && npm run prepare:babel && npm run prepare:prettier",
    "prepare:clean": "rm -rf public private test",
    "prepare:babel": "babel src -d . --keep-file-extension",
    "prepare:prettier": "prettier --write public private test",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:api && npm run test:size",
    "test:eslint": "eslint --ext mjs,js .",
    "test:prettier": "prettier -c .",
    "test:api": "coverage-node test/index.mjs",
    "test:size": "size-limit",
    "dev": "chokidar src --initial -c 'npm run prepare && npm run test:api --silent'",
    "prepublishOnly": "npm test"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@size-limit/preset-small-lib": "^4.11.0",
    "@testing-library/react": "^11.2.7",
    "chokidar-cli": "^2.1.0",
    "coverage-node": "^5.0.1",
    "eslint": "^7.27.0",
    "eslint-config-env": "^20.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsdoc": "^35.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.11.0",
    "snapshot-assertion": "^3.0.0",
    "test-director": "^6.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@emotion/react": "^11.4.0",
    "deepmerge": "^4.2.2",
    "facepaint": "^1.2.1",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  }
}
